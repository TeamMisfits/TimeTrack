<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.timetrack">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="TimeTrack">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".CreateClass"
            android:label="Create Class" />
        <activity
            android:name=".DisplayClass"
            android:label="Display Class" />
        <activity
            android:name=".CreateTask"
            android:label="Create Task " />
        <activity android:name=".DisplayTask"
            android:label="View Task" />
        <activity android:name=".DisplayData"
            android:label="All Data" />
        <activity android:name=".ClassData"
            android:label="Class Data "></activity>
    </application>

</manifest>